{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "version": "1.7.5",
  "name": "mine-cloudvod/video-playlist",
  "title": "Video Playlist",
  "category": "mine",
  "icon": "",
  "description": "Video Playlist of Cloud VoD.",
  "keywords": [
    "mine",
    "cloud",
    "vod",
    "video",
    "list",
    "play"
  ],
  "textdomain": "mine-cloudvod",
  "attributes": {
    "plName": {
      "type": "string"
    },
    "mcvTag": {
      "type": "number"
    },
    "show": {
      "type": "boolean",
      "default": false
    },
    "autonext": {
      "type": "boolean",
      "default": true
    }
  },
  "render": "file:./render.php",
  "editorScript": [
    "file:./index.js",
    "file:./view.js"
  ]
}