{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "version": "1.7.5",
  "name": "mine-cloudvod/course-list",
  "title": "Course List",
  "category": "mine",
  "icon": "editor-table",
  "description": "Show some course data",
  "keywords": [
    "mine",
    "course",
    "list"
  ],
  "textdomain": "mine-cloudvod",
  "attributes": {
    "categories": {
      "type": "number"
    },
    "tags": {
      "type": "number"
    },
    "title": {
      "type": "string"
    },
    "template": {
      "type": "string"
    },
    "morelink": {
      "type": "string"
    },
    "moretext": {
      "type": "string",
      "default": "More"
    },
    "rows": {
      "type": "number",
      "default": 1
    },
    "columns": {
      "type": "number",
      "default": 4
    }
  },
  "render": "file:./render.php",
  "editorScript": "file:./index.js",
  "editorStyle": [
    "file:./index.css"
  ]
}